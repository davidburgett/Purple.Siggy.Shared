    <ImplicitUsings>enable</ImplicitUsings>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId>Purple.Siggy.Shared</PackageId>
    <Title>Purple.Siggy.Shared</Title>
    <Authors>Purple Siggy</Authors>
    <Company>Purple Siggy</Company>
    <Product>Purple.Siggy.Shared</Product>
    <PackageProjectUrl>https://github.com/davidburgett/Purple.Siggy.Shared</PackageProjectUrl>
